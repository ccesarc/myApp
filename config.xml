<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="br.com.divullg.testegeral" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Teste Geral</name>
  <description>Testando muita coisa.</description>
  <author email="caiocesarca@gmail.com" href="http://divullg.com.br/">Ionic Framework Team</author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-navigation href="http://ionic.local/*"/>
  <allow-navigation href="http://192.168.214.2:8100"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <preference name="android-windowSoftInputMode" value="adjustResize"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon density="ldpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="land-hdpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="land-ldpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="land-mdpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="land-xhdpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="port-hdpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="port-ldpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="port-mdpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <splash density="port-xhdpi" src="resources\android\splash\drawable-land-ldpi-screen.png"/>
    <preference name="Orientation" value="portrait"/>
    <preference name="LoadingDialog" value="Porfavor aguarde, app carregando"/>
    <preference name="LoadingPageDialog" value=" Porfavor aguarde 2, app carregando"/>
    <preference name="Fullscreen" value="false"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <plugin name="cordova-plugin-whitelist" spec="1.3.1"/>
  <plugin name="cordova-plugin-console" spec="1.0.5"/>
  <plugin name="cordova-plugin-statusbar" spec="2.2.1"/>
  <plugin name="cordova-plugin-device" spec="1.1.4"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.1"/>
  <plugin name="phonegap-plugin-push" spec="~1.5.2">
    <variable name="SENDER_ID" value="495060840000"/>
  </plugin>
  <plugin name="cordova-plugin-geolocation" spec="~2.4.2"/>
  <engine name="android" spec="~6.0.0"/>
</widget>